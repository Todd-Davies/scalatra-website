#import(uk.co.todddavies.app.models.NoteCollection)

<%@ val notes: List[NoteCollection] %>

#for (i <- notes)
    #if(i.standard)
        <a href="${i.getResourcePath()}">${i.name}</a>
    #end
    #if(i.kindle)
        <a href="${i.getKindleResourcePath()}">kindle</a>
    #end
    #if(i.flashcards)
        <a href="${i.getFlashcardsResourcePath()}">flashcards</a>
    #end
#end